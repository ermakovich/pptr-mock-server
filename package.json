{
  "name": "pptr-mock-server",
  "version": "1.0.0",
  "description": "Tiny library for backendless testing using Puppeteer",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist --ignore '**/*.test.js'",
    "prepublish": "npm run build",
    "test": "jest",
    "test-ci":
      "jest --coverage --ci --runInBand --reporters=jest-junit --coverageReporters=text-lcov | coveralls",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint .",
    "lint-ci": "eslint . --format junit -o reports/junit/lint-results.xml",
    "docs": "documentation build src/index.js -f md -o api.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getdock/pptr-mock-server.git"
  },
  "keywords": ["automated-tests", "puppeteer"],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getdock/pptr-mock-server/issues"
  },
  "homepage": "https://github.com/getdock/pptr-mock-server#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "dev-configs": "^1.0.3",
    "documentation": "^8.0.2",
    "eslint": "^5.1.0",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^23.4.1",
    "jest-junit": "^5.1.0",
    "prettier": "1.13.6",
    "puppeteer": ">= 1.5"
  },
  "peerDependencies": {
    "puppeteer": ">= 1.5"
  },
  "jest-junit": {
    "output": "./reports/junit/test-results.xml"
  }
}
